{
  "name": "charged-meteorite",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --port 3000",
    "dev:webpack": "next dev --webpack",
    "dev:clean": "taskkill /F /IM node.exe 2>nul & next dev --webpack",
    "kill": "taskkill /F /IM node.exe 2>nul || echo Nenhum processo Node encontrado",
    "restart": "taskkill /F /IM node.exe 2>nul & timeout /t 2 /nobreak >nul & next dev --webpack",
    "build": "next build",
    "start": "next start",
    "lint": "eslint",
    "validate:prices": "node scripts/validate-prices.js",
    "test:gemini": "npx tsx scripts/test-gemini.ts",
    "validate:product": "npx tsx scripts/validate-product.ts",
    "integrity:products": "npx tsx scripts/integrity-products.ts",
    "integrity:sitemap": "npx tsx scripts/integrity-sitemap.ts",
    "integrity:routes": "npx tsx scripts/integrity-routes.ts",
    "integrity:links": "npx tsx scripts/integrity-links.ts",
    "integrity:ui-contracts": "npx tsx scripts/integrity-ui-contracts.ts",
    "integrity": "npm run integrity:products && npm run integrity:sitemap && npm run integrity:routes && npm run integrity:links && npm run integrity:ui-contracts",
    "test:integrity": "start-server-and-test start http://localhost:3000 integrity",
    "integrity:local": "cross-env INTEGRITY_BASE_URL=http://localhost:3000 npm run integrity",
    "integrity:prod": "cross-env INTEGRITY_BASE_URL=https://comparatop.com.br npm run integrity:sitemap && cross-env INTEGRITY_BASE_URL=https://comparatop.com.br npm run integrity:links",
    "test:redirects": "npx tsx scripts/test-redirects.ts",
    "test:lighthouse": "npx tsx scripts/run-lighthouse.ts",
    "analyze": "cross-env ANALYZE=true next build --webpack",
    "scaffold:product": "npx tsx scripts/scaffold-product.ts",
    "scaffold:dry": "npx tsx scripts/scaffold-dry.ts",
    "validate:completeness": "npx tsx scripts/validate-product-completeness.ts",
    "census": "npx tsx scripts/category-census.ts",
    "integrity:placeholders": "npx tsx scripts/integrity-placeholders.ts",
    "validate:pdp-mocks": "npx tsx scripts/validate-pdp-mocks.ts",
    "fix:pdp-mocks": "npx tsx scripts/fix-pdp-mocks.ts",
    "validate:pdp-viewmodel": "npx tsx scripts/validate-pdp-viewmodel.ts",
    "validate:pdp-completeness": "npx ts-node --esm scripts/validate-pdp-completeness.ts",
    "validate:pdp-content": "npx ts-node --esm scripts/validate-pdp-content.ts",
    "qa:category": "npx tsx scripts/qa-category.ts",
    "qa:robot-vacuums": "npx tsx scripts/qa-category.ts --category robot-vacuum",
    "qa:robot-vacuums:update": "npx tsx scripts/qa-category.ts --category robot-vacuum --write-snapshot",
    "new:category": "npx tsx scripts/new-category-kit.ts",
    "qa:all": "npx tsx scripts/qa-all.ts",
    "gate:qa": "cross-env STRICT_SPECS=true npm run qa:all",
    "validate:scores": "npx tsx scripts/validate-scores.ts",
    "generate:score-baseline": "npx tsx scripts/generate-score-baseline.ts",
    "compare:score-baseline": "npx tsx scripts/compare-score-baseline.ts",
    "typecheck": "npx tsc --noEmit --skipLibCheck",
    "gate:full": "npm run validate:scores && npm run gate:qa && npm run typecheck",
    "gate": "npm run gate:qa",
    "test:e2e": "npx playwright test"
  },
  "dependencies": {
    "@ai-sdk/google": "^3.0.6",
    "@ai-sdk/openai": "^3.0.7",
    "@ai-sdk/react": "^2.0.25",
    "@ai-sdk/rsc": "^2.0.25",
    "@google/generative-ai": "^0.24.1",
    "@libsql/client": "^0.15.15",
    "@prisma/adapter-libsql": "^7.2.0",
    "@prisma/client": "^5.22.0",
    "@radix-ui/react-dialog": "^1.1.15",
    "@react-email/components": "^1.0.6",
    "@supabase/supabase-js": "^2.90.1",
    "@tanstack/react-table": "^8.21.3",
    "@vercel/analytics": "^1.6.1",
    "@vercel/kv": "^3.0.0",
    "@vercel/og": "^0.8.6",
    "ai": "^6.0.25",
    "cheerio": "^1.1.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "framer-motion": "^12.24.0",
    "got": "^14.6.6",
    "json-rules-engine": "^7.3.1",
    "lucide-react": "^0.562.0",
    "next": "16.1.1",
    "openai": "^6.15.0",
    "prisma": "^5.22.0",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "recharts": "^3.6.0",
    "resend": "^6.8.0",
    "tailwind-merge": "^3.4.0",
    "uuid": "^13.0.0",
    "web-vitals": "^5.1.0",
    "zod": "^4.3.5"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^16.1.3",
    "@playwright/test": "^1.58.0",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20.19.30",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/uuid": "^10.0.0",
    "cross-env": "^7.0.3",
    "dotenv": "^17.2.3",
    "eslint": "^9",
    "eslint-config-next": "16.1.1",
    "start-server-and-test": "^2.0.5",
    "tailwindcss": "^4.1.18",
    "tsx": "^4.21.0",
    "typescript": "^5",
    "vitest": "^4.0.17"
  }
}